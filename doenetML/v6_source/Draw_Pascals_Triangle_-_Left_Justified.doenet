    <circle name="circ0_0" center="(0,18)" fixLocation/>
    <label anchor="$circ0_0.center" fixLocation>1</label>
    <lineSegment name="row0bottom" endpoints="(-2,16) (32,16)" fixLocation styleNumber="5"/>
    <label anchor="$row0bottom.endpoints[2]" positionFromAnchor="upperleft">Row 0</label>
  </group>

  <lineSegment name="col0Right" endpoints="(2,-10) (2,20)" fixLocation styleNumber="6"/>
  <label anchor="$col0Right.endpoints[1] - (1,-0.5)" positionFromAnchor="left" fixLocation>Col 0</label>
  
