<problem><title/>
<setup>
  <selectFromSequence name="h" from="-4" to="4"/>
  <selectFromSequence name="k" from="-4" to="4"/>
  <selectFromSequence name="a" from="1" to="4"/>
  <selectFromSequence name="b" from="1" to="4"/>
  <selectFromSequence name="c" from="2" to="4"/>
  <math name="t0">0</math>
  <math name="t1" simplify>pi/(2$c)</math>
  <math name="t2" simplify>pi/($c)</math>
  <math name="t3" simplify>3pi/(2$c)</math>
  <math name="t4" simplify>2pi/($c)</math>
  <math name="x0" simplify>$h + $a</math>
  <math name="y0" simplify>$k</math>
  <point name="p0" styleNumber="5" labelPosition="right">($x0, $y0)
    <label>$p0.coords</label>
  </point>

  <math name="x1" simplify>$h</math>
  <math name="y1" simplify>$k + $b</math>
  <point name="p1" styleNumber="5" labelPosition="top">($x1, $y1)
    <label>$p1.coords</label>
  </point>

  <math name="x2" simplify>$h - $a</math>
  <math name="y2" simplify>$k</math>
  <point name="p2" styleNumber="5" labelPosition="left">($x2, $y2)
    <label>$p2.coords</label>
  </point>

  <math name="x3" simplify>$h</math>
  <math name="y3" simplify>$k - $b</math>
  <point name="p3" styleNumber="5" labelPosition="bottom">($x3, $y3)
    <label>$p3.coords</label>
  </point>

  
  <math name="xOft" simplify="numberspreserveorder">$h + $a cos($c t)</math>
  <math name="yOft" simplify="numberspreserveorder">$k + $b sin($c t)</math>

  <number name="xmin">min(-1.5, $p0.x - 1.5, $p1.x - 1.5, $p2.x - 1.5, $p3.x - 1.5)</number>
  <number name="xmax">max(1.5, $p0.x + 1.5, $p1.x + 1.5, $p2.x + 1.5, $p3.x + 1.5)</number>
  <number name="ymin">min(-1.5, $p0.y - 1.5, $p1.y - 1.5, $p2.y - 1.5, $p3.y - 1.5)</number>
  <number name="ymax">max(1.5, $p0.y + 1.5, $p1.y + 1.5, $p2.y + 1.5, $p3.y + 1.5)</number>
</setup>


<!--testing-->
