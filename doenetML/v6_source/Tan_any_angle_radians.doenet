<problem>
<setup>
  
  <select name="a">-pi/3 -pi/4 -pi/6 0 pi/6 pi/4 pi/3 2pi/3 3pi/4 5pi/6 pi 7pi/6 5pi/4 4pi/3 5pi/3 7pi/4 11pi/6 2pi pi/2 -pi/2 3pi/2</select>

  <math name="expr">tan($a)</math>
  <number name="solN">tan($a)</number>
  <math name="sol">
  <conditionalContent>
    <case condition="tan($a) = tan(0)">0</case>
    <case condition="tan($a) = tan(pi/6)">sqrt(3)/3</case>
    <case condition="tan($a) = tan(pi/4)">1</case>
    <case condition="tan($a) = tan(pi/3)">sqrt(3)</case>
    <case condition="tan($a) = tan(2pi/3)">-sqrt(3)</case>
    <case condition="tan($a) = tan(3pi/4)">-1</case>
    <case condition="tan($a) = tan(5pi/6)">-sqrt(3)/3</case>
    <case condition="$a = pi/2 or $a = -pi/2 or $a = 3pi/2">undefined</case>
  </conditionalContent>
  </math>
  <matchesPattern name="same" pattern="tan( )">$userAns</matchesPattern>
</setup>

<!--testing-->
<!-- <p>a = $a</p>
<p>expr = $expr</p>
<p>sol = $sol</p>
<p>solN = $solN</p> -->
<!--testing-->

  
<p>Find $expr.  Enter your answer as an exact value using <alert>square roots</alert><footnote>To enter a square root symbol, type "sqrt"</footnote> when necessary.  Do not use a calculator to give a decimal approximation.</p>


<p><m>$expr = </m><answer name="ans1">
      <mathInput name="userAns"/>
      <award name="correct" allowedErrorInNumbers="0.00000000001" allowedErrorIsAbsolute>
        <when>
          $userAns = $sol and !$same
        </when>
      </award>
      <award name="numerical" allowedErrorInNumbers="0.01" allowedErrorIsAbsolute credit="0">   
        <when>
          $userAns = $sol and !$same
        </when>
      </award>
    </answer></p>

<p><em>Note, if the tangent of the angle is undefined, type "undefined".</em></p>




