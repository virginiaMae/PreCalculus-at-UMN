    <option>Nice work!</option>
    <option>Amazing!</option>
    <option>Excellent!</option>
    <option>Right on!</option>
    <option>Perfect!</option>
  </select>
</setup>
  <feedback condition="$correct and $ans1.creditAchieved = 1">$correctFeedback</feedback>

  <feedback condition="$angleBCorrect = false and $ans1.justSubmitted">Angle <m>B</m> is incorrect.</feedback>

  <feedback condition="$angleCCorrect = false and $ans1.justSubmitted">Angle <m>C</m> is incorrect.</feedback>

  <feedback condition="$sideCorrect = false and $ans1.justSubmitted">Side length <m>a</m> is incorrect.</feedback>


