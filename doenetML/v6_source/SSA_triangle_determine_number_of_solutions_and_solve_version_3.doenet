<!--testing-->


<p>This is a two-part problem.</p>

<sideBySide>  
<div>
<p>In the triangle shown (not drawn to scale):</p>
  <md>
    <mrow>A &= $A^{\circ}</mrow>
    <mrow>a &= $a</mrow>
    <mrow>c &= $c</mrow>
  </md>

  <p><alert>Part I:</alert> How many solutions exist for this triangle?</p>

    <choiceInput name="ci1">
      <choice name="noSol" credit="$solN = 0">No solutions</choice>
      <choice name="oneSol" credit="$solN = 1">One solution</choice>
      <choice name="twoSol" credit="$solN = 2">Two solutions</choice>
    </choiceInput>
</div>

 
<graph name="g1" size="small" fixAxes displayXAxis="false" displayYAxis="false" xmin="-3" xmax="13" ymin="-2" ymax="9" identicalAxisScales>
  <point name="pA" fixed labelPosition="left" styleNumber="5">(0,0)
    <label><m>A</m></label>
  </point>
  <point name="pB" fixed styleNumber="5" labelPosition="top">(4, 6)
    <label><m>B</m></label>
  </point>
  <point name="pC" fixed styleNumber="5" labelPosition="right">(10,0)
    <label><m>C</m></label>
  </point>
  <lineSegment name="aSeg" styleNumber="2" endpoints="$pB $pC">
    <label><m>a</m></label>
  </lineSegment>
  <lineSegment name="bSeg" styleNumber="3" endpoints="$pA $pC" labelPosition="lowerright">
    <label><m>b</m></label>
  </lineSegment>
  <lineSegment name="cSeg" styleNumber="4" endpoints="$pA $pB" labelPosition="upperleft">
    <label><m>c</m></label>
  </lineSegment>
  
  <angle name="angleA" through="$pB $pA $pC" styleNumber="2"/>

  <angle name="angleB" through="$pA $pB $pC" styleNumber="3" />
  
  <angle name="angleC" through="$pA $pC $pB" styleNumber="4" />

  <label anchor="(9.5,6.5)">Not to scale</label>
</graph>
</sideBySide>
 


<!--*********************************************
            User Input
**********************************************-->
<p><alert>Part II:</alert><nbsp/><nbsp/><nbsp/>  <em hide="$ci1.selectedIndices = 1 or $ci1.selectedIndices = 2 or $ci1.selectedIndices = 3">Answer Part I. to proceed to Part II.</em>  
  <nbsp/><nbsp/><nbsp/>  <em hide="!($ci1.selectedIndices = 2 or $ci1.selectedIndices = 3)">Round angles to the nearest <m>0.01^{\circ}</m> and lengths to the nearest <m>0.01</m>.</em></p>
  <conditionalContent>
     <case condition="$ci1.selectedIndices = 1">
       <answer name="ans1NoSol" >
         <award name="correct0"><when>$solN = 0</when></award>
       </answer>
     </case>
     <case condition="$ci1.selectedIndices = 2 or $ci1.selectedIndices = 3">
  <sideBySide>
    <div>
      <p><alert>Solution 1:</alert></p>
      <p><m>B_1 = </m><mathInput name="userB2sol1" /><m>^{\circ}</m></p>
      <p><m>C_1 = </m><mathInput name="userC2sol1" /><m>^{\circ}</m></p>
      <p><m>b_1 = </m><mathInput name="userbsol1"/></p>
    </div>
    <div hide="$ci1.selectedIndices = 2">
      <p><alert>Solution 2:</alert></p>
      <p><m>B_2 = </m><mathInput name="userB2sol2" /><m>^{\circ}</m></p>
      <p><m>C_2 = </m><mathInput name="userC2sol2" /><m>^{\circ}</m></p>
      <p><m>b_1 = </m><mathInput name="userbsol2" /></p>
    </div>
  </sideBySide>
       
<setup>
  <mathList name="zeroSol">(0,0,0) (0,0,0)</mathList>
  <vector name="userSol1">
    ($userB2sol1, $userC2sol1, $userbsol1)</vector>
  <vector name="userSol2">
    ($userB2sol2, $userC2sol2, $userbsol2)</vector>
  <mathList name="userSols" unordered>
    $userSol1 $userSol2
  </mathList>
  <vector name="sol2_1">($Bn, $Cn, $bn)</vector>
  <vector name="sol2_2">($Balt, $Calt, $balt)</vector>
  <mathList name="sol2List" unordered>
    $sol2_1 $sol2_2</mathList>
  <boolean name="errorInSol1" allowedErrorInNumbers="0.1" allowedErrorIsAbsolute>$userSol1 != $sol2_1 and $userSol1 != $sol2_2</boolean>
  <boolean name="errorInSol2" allowedErrorInNumbers="0.1" allowedErrorIsAbsolute>$userSol2 != $sol2_1 and $userSol2 != $sol2_2</boolean>
</setup>


<!-- testing -->
<!-- <p>userSol1 = $userSol1</p>
<p>userSol2 = $userSol2</p>
<p>userSols = $userSols</p>
<p>sol2_1 = $sol2_1</p>
<p>sol2_2 = $sol2_2</p>
<p>sol2List = $sol2List</p> -->
<!-- /testing -->
       
<answer name="ans1" allowedErrorInNumbers="0.1" allowedErrorIsAbsolute>
  <award name="correct1">
    <when>$solN = 1 and $ci1.selectedIndices = 2 and $userSol1 = $sol2_1</when>
  </award>
  <award name="correct2">
    <when>$solN = 2 and $userSols = $sol2List</when>
  </award>
</answer>
     </case>
  </conditionalContent>

  
