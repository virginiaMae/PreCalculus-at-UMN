<!--************************************************************
Author: University of Minnesota PreCalculus Dept.
Reviewed/Remixed by: Doenet team

About the problem: Multiple choice, determine the cosine of a standard unit circle
angle for large and negative angles in degrees. Unit circle widget provided.
*************************************************************-->

<problem>
<setup>
   <row name="ri1">
    <cell><p><booleanInput name="bi1">
      <label><m>\displaystyle{$choices.displacement[1]}</m></label></booleanInput></p>
    </cell>
    <cell><p><booleanInput name="bi4">
      <label><m>\displaystyle{$choices.displacement[4]}</m></label></booleanInput></p>
    </cell>
      <cell><p><booleanInput name="bi7">
      <label><m>\displaystyle{$choices.displacement[7]}</m></label></booleanInput></p>
    </cell>
  </row>
  <row name="ri2">
    <cell><p><booleanInput name="bi2">
      <label><m>\displaystyle{$choices.displacement[2]}</m></label></booleanInput></p>
    </cell>
    <cell><p><booleanInput name="bi5">
      <label><m>\displaystyle{$choices.displacement[5]}</m></label></booleanInput></p>
    </cell>
    <cell><p><booleanInput name="bi8">
      <label><m>\displaystyle{$choices.displacement[8]}</m></label></booleanInput></p>
    </cell>
  </row>
  <row name="ri3">
    <cell><p><booleanInput name="bi3">
      <label><m>\displaystyle{$choices.displacement[3]}</m></label></booleanInput></p>
    </cell>
    <cell><p><booleanInput name="bi6">
      <label><m>\displaystyle{$choices.displacement[6]}</m></label></booleanInput></p>
    </cell>
    <cell><p><booleanInput name="bi9">
      <label><m>\displaystyle{$choices.displacement[9]}</m></label></booleanInput></p>
    </cell>
  </row>
  
  <select name="a">120 135 150 180 210 225 240 300 315 330 360 390 405 420 450 480 495 510 540 570 585 600 630 660 675 690 720 750 765 780 810 840 855 870 900 930 945 960 990 1020 1035 1050 1080</select>
  <select name="b">-1 1</select>
  <math name="angleDisp" simplify>$b $a</math>
  <math name="angle" simplify>$angleDisp (pi/180)</math>
  <math name="expr" format="latex">\cos($angleDisp^{\circ})</math>
  <number name="solN">sin($angle)</number>

  <boolean name="numPi_6" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(pi/6)</boolean>
  <boolean name="numPi_4" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(pi/4)</boolean>
  <boolean name="numPi_3" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(pi/3)</boolean>
  <boolean name="numPi_2" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(pi/2)</boolean>
  <boolean name="num2Pi_3" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(2pi/3)</boolean>
  <boolean name="num3Pi_4" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(3pi/4)</boolean>
  <boolean name="num5Pi_6" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(5pi/6)</boolean>
  <boolean name="numPi" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(pi)</boolean>
  <boolean name="numPi_0" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>cos($angle) = cos(0)</boolean>
  
  <math name="sol">  
  <conditionalContent>
    <case condition="$numPi_6">sqrt(3)/2</case>
    <case condition="$numPi_4">sqrt(2)/2</case>
    <case condition="$numPi_3">1/2</case>
    <case condition="$numPi_2">0</case>
    <case condition="$num2Pi_3">-1/2</case>
    <case condition="$num3Pi_4">-sqrt(2)/2</case>
    <case condition="$num5Pi_6">-sqrt(3)/2</case>
    <case condition="$numPi">-1</case>
    <case condition="$numPi_0">1</case>
  </conditionalContent>
  </math>
<vector name="choices">
  (-1, -sqrt(3)/2, -sqrt(2)/2, -1/2, 0, 1/2, sqrt(2)/2, sqrt(3)/2, 1)
</vector>      
  
</setup>
  
<!--testing-->
<!-- <p>angleDisp = $angleDisp</p>
<p>sol = $sol</p>
<p>choices.displacement[1] = $choices.displacement[1]</p> 
<p>Answer check: <boolean>cos($angleDisp pi/180) = $sol</boolean></p>
<p>correctSelected = $correctSelected{hide='false'}</p> -->
<!--testing-->



<tabular top="minor" bottom="minor" left="minor" right="minor">
  <row><cell halign="center" colSpan="3"><p><m>$expr = </m></p></cell></row>
  <row copySource="ri1"/>
  <row copySource="ri2"/>
  <row copySource="ri3"/>
</tabular>

<boolean name="correctSelected" hide>
  <conditionaLContent>
    <case condition="$sol = $choices.displacement[1]">$bi1 = true and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[2]">$bi1 = false and $bi2 = true and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[3]">$bi1 = false and $bi2 = false and $bi3 = true and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[4]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = true and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[5]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = true and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[6]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = true and $bi7 = false and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[7]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = true and $bi8 = false and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[8]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = true and $bi9 = false</case>
    <case condition="$sol = $choices.displacement[9]">$bi1 = false and $bi2 = false and $bi3 = false and $bi4 = false and $bi5 = false and $bi6 = false and $bi7 = false and $bi8 = false and $bi9 = true</case>
  </conditionaLContent>
</boolean>
<answer name="ans1">
  <award><when>$correctSelected</when></award>
</answer>


