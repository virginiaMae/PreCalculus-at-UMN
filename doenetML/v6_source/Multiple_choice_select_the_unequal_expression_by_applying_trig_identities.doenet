<problem>
<setup>
<selectFromSequence name="a" from="10" to="350" step="10"/>
<math name="m0">cos($a pi/180)</math>
<math name="mSol">2 cos($halfAngle pi/180)</math>
<math name="m1">cos($b pi/180)</math>
<math name="m2">cos($c pi/180)</math>
<math name="m3">2 cos^2 ($halfAngle pi/180) - 1</math>
<math name="m4">sin($d pi/180)</math>
  
<math name="halfAngle" simplify>$a / 2</math>
<math name="choiceSol">2 cos($halfAngle)</math>
<math name="b" simplify>$a + 360</math>
<math name="choice1">cos($b)</math>
<math name="c" simplify>360 - $a</math>
<math name="choice2">cos($c)</math>
<math name="choice3">2 cos^2 ($halfAngle) - 1</math>
<math name="d" simplify>90 - $a</math>
<math name="choice4">sin($d)</math>
<boolean name="check1" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>$m0 = $mSol</boolean>
<boolean name="check2" allowedErrorInNumbers="0.0000001" allowedErrorIsAbsolute>$m0 = $m1 and $m0 = $m2 and $m0 = $m3 and $m0 = $m4</boolean>
</setup>

