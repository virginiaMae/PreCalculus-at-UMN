<!--testing-->

<p>Which of the following is <alert><em>not</em></alert> equal to <m>\cos($a^{\circ})</m> ?</p>
 
<p>
  <answer name="ans1" maxNumAttempts="1" shuffleOrder>
    <choice>$choice1</choice>
    <choice>$choice2</choice>
    <choice>$choice3</choice>
    <choice credit="1">$choiceSol</choice>
    <choice>$choice4</choice>
  </answer>
</p>

  
